import random
import sys
ask_again="yes"
while (ask_again=="yes"):
  if(ask_again=="no"):
    exit()
  ans=random.randint(0,8)
  question=input("ENTER YOUR QUESTION: ")
  if(ans==0):
    print("smart work matters")
  elif(ans==1):
    print("that might not be the correct way")
  elif(ans==2):
    print("repeat again!")
  elif(ans==3):
    print("work hard you will achieve!")
  elif(ans==4):
    print("be active and work on it")
  elif(ans==5):
    print("be concentrated you will achieve")
  elif(ans==6):
    print("ups and down's are common in life")
  elif(ans==7):
    print("be patient you will definitely win")
  elif(ans==8):
    print("only one life be happy ")
  ask_again=input("click yes toask again:")
  
